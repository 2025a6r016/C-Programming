#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

      int days,years,weeks,totalday,remaining;
    scanf("%d", &totalday);
    years = totalday / 365;
    remaining = totalday % 365;
    weeks = remaining / 7;
    days = remaining % 7;
    printf("Years: %d\n",years);
    printf("Weeks: %d\n",weeks);
    printf("Days: %d\n",days);
    
    
    return 0;
}
